    "import requests\n",
    "\n",
    "payload = {\n",
    "    'q': 'banana'\n",
    "}\n",
    "\n",
    "url = f\"https://api-instagram.ms.codigoaberto.io/instagram/search/\"\n",
    "response = requests.get(url, params=payload)\n",
    "data = response.json()\n",
    "\n",
    "for item in data['items']:\n",
    "    print(f\"#{item['name']}\", end=' ')\n"
   ]
  }
